{
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "outDir": "temp/compiledTypeScript",
    "target": "es3"
  }
}
